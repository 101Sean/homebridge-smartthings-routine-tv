{
  "name": "homebridge-smartthings-routine-tv",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": ["homebridge-plugin"],
  "engines": {
    "homebridge": ">=1.0.0"
  },
  "dependencies": {
    "axios": "^1.5.0"
  },
  "homebridge": {
    "accessories": [
      {
        "accessory": "StRoutineTV",
        "name": "TV Routine",
        "schema": {
          "type": "object",
          "required": ["name","token","routineId"],
          "properties": {
            "name":      { "type": "string", "default": "TV Routine" },
            "token":     { "type": "string" },
            "routineId": { "type": "string" }
          }
        }
      }
    ]
  }
}
