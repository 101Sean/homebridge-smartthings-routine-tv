{
  "name": "homebridge-smartthings-routine-tv",
  "version": "1.0.6",
  "main": "index.js",
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "smartthings",
    "tv",
    "routine"
  ],
  "engines": {
    "homebridge": ">=1.0.0"
  },
  "dependencies": {
    "axios": "^1.5.0"
  },
  "homebridge": {
    "accessories": [
      {
        "accessory": "homebridge-smartthings-routine-tv",
        "schema": {
          "type": "object",
          "required": ["name","token","sceneId"],
          "properties": {
            "name":    { "type":"string", "default":"TV Routine" },
            "token":   { "type":"string" },
            "sceneId": { "type":"string" }
          }
        }
      }
    ]
  }
}
